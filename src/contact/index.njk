 <p>CONTACT TEST 123</p>


<form class="contact-form" id="contactForm" method="post" action="/api/contact">
  <label for="name">name *</label>
  <input id="name" name="name" required autocomplete="name" />

  <label for="email">e-mail *</label>
  <input id="email" name="email" type="email" required autocomplete="email" />

  <label for="message">message *</label>
  <textarea id="message" name="message" required rows="7"></textarea>

  <!-- honeypot (bots fill this). Humans leave it empty -->
  <input name="website" tabindex="-1" autocomplete="off" style="position:absolute;left:-9999px" aria-hidden="true" />

  <button type="submit">send</button>

  <p class="contact-status" id="contactStatus" aria-live="polite"></p>
</form>
